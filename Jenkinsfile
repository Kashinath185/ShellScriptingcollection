pipeline {
  agent any
  stages {
    stage('Stage1') {
      steps {
        sh 'echo "This is stage one"'
      }
    }

    stage('Stage2') {
      steps {
        sh 'echo "This is second stage"'
      }
    }

    stage('Stage3') {
      steps {
        sh 'echo "This is third stage"'
      }
    }

  }
}